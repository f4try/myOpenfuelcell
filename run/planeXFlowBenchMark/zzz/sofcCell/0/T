    }

    airOutlet
    {
        type            zeroGradient;
    }

    airSides
    {
        type            zeroGradient;
    }

    interconnectTop
    {
        type            zeroGradient;
    }

    interconnectBottom
    {
        type            zeroGradient;
    }

    interconnectSides
