FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          cellProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

anodePatch      anode;
cathodePatch    cathode;

voltage         V [1 2 -3 0 0 -1 0]  0.75;
// 0.9335;

E0              E0 [1 2 -3 0 0 -1 0]  0.997;

// prescribed mean current density
ibar0           ibar0 [0 -2 0 0 0 1 0] 4000.0;

// voltage correction relaxation coefficient
Rhat            Rhat [1 4 -3 0 0 -2 0] 1.0e-5;
//Rhat            Rhat [1 4 -3 0 0 -2 0] 1.0e-6;

