FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

vertices
(
// Interconnect Air
    (0 0 1.016)
    (90 0 1.016)
    (90 82 1.016)
    (0 82 1.016)
// Air Electrolyte
    (0 0 1.905)
    (90 0 1.905)
    (90 82 1.905)
    (0 82 1.905)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 20 10) simpleGrading (1 1 1)
);

edges
(
);

patches
(
    patch airInlet
    (
        (0 1 5 4)
    )

    patch airOutlet
    (
        (3 7 6 2)
    )

    patch airSides
    (
        (0 4 7 3)
        (1 2 6 5)
    )

    patch lowerInterconnect
    (
        (0 3 2 1)
    )

    patch cathode
    (
        (4 5 6 7)
    )
);

mergePatchPairs
(
);

