FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSolution;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    T       BICCG 1e-9 0;

    T+T+T+T BiCGStab
    {
        preconditioner
        {
            type                Cholesky;
        }

         minIter      0;
         maxIter      1000;
         tolerance    1e-9;
         relTol       0.0;
    };
}

PISO
{
    nCorrectors     2;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
}

relaxationFactors
{
    T               0.99;
}


