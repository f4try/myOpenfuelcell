        end             (0.00225 0.03895 0.0009144);
	nPoints         5;
    }

    lineX2
    {
        type            uniform;
        axis            xyz;
        //start           (0.00225 0.03895 0.0041148);
        //end             (0.00225 0.03895 0.0051308);
        start           (0.00225 0.03895 0.0042164);
        end             (0.00225 0.03895 0.0050292);
	nPoints         5;
    }
);

surfaces
(
    /********
    interpolatedPlane
    {
        type            plane;
