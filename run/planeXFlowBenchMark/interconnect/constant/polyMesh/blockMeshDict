FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

vertices
(
// bottom
    (0 0 0)
    (90 0 0)
    (90 82 0)
    (0 82 0)
// Interconnect Air
    (0 0 1.016)
    (90 0 1.016)
    (90 82 1.016)
    (0 82 1.016)
// Interconnect Fuel
    (0 0 4.1148)
    (90 0 4.1148)
    (90 82 4.1148)
    (0 82 4.1148)
// top
    (0 0 5.1308)
    (90 0 5.1308)
    (90 82 5.1308)
    (0 82 5.1308)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (20 20 5) simpleGrading (1 1 1)
    hex (8 9 10 11 12 13 14 15) (20 20 5) simpleGrading (1 1 1)
);

edges
(
);

patches
(
    patch interconnectSides
    (
        (0 1 5 4)
        (3 7 6 2)
        (0 4 7 3)
        (1 2 6 5)
        (8 9 13 12)
        (11 15 14 10)
        (8 12 15 11)
        (9 10 14 13)
    )

    patch interconnectBottom
    (
        (0 3 2 1)
    )

    patch interconnectTop
    (
        (12 13 14 15)
    )

    patch interconnectAir
    (
        (4 5 6 7)
    )

    patch interconnectFuel
    (
        (11 10 9 8)
    )
);

mergePatchPairs
(
);

