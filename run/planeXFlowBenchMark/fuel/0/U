FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.572 0 0);

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           uniform (0.572 0 0);
    }

    fuelOutlet
    {
        type            zeroGradient;
    }

    fuelSides
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    anode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    upperInterconnect
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


