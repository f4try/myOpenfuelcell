FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           volScalarField;
    object          yH2O;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.05;

boundaryField
{
    fuelInlet
    {
        type            fixedValue;
        value           uniform 0.05;
    }

    fuelOutlet
    {
        type            zeroGradient;
    }

    fuelSides
    {
        type            zeroGradient;
    }

    anode
    {
        type            fixedGradient;
        gradient        uniform 0;
        value           uniform 0.05;
    }

    upperInterconnect
    {
        type            zeroGradient;
    }
}


