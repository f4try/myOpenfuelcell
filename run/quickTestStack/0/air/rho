FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      rho;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 0.3516;

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           uniform 0.3516;
    }
    airOutlet
    {
        type            zeroGradient;
    }
    air_to_electrolyte
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
    air_to_interconnect
    {
        type            fixedGradient;
        gradient        uniform 0;
    }
}


