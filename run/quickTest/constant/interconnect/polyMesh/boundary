(
    interconnect
    {
        type            patch;
        nFaces          1920;
        startFace       51600;
    }
    interconnectSides
    {
        type            patch;
        nFaces          3360;
        startFace       53520;
    }
    interconnect_to_air
    {
        type            mappedWall;
        nFaces          3360;
        startFace       56880;
