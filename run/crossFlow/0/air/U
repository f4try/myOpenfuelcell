FoamFile
{
    version         2.0;
    format          ascii;
    class           volVectorField;
    object          U;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 1 0);

boundaryField
{

    airInlet
    {
        type            fixedValue;
        value           uniform (0 1 0);
    }

    airOutlet
    {
        type            zeroGradient;
    }

    airSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }
    cathodeSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }    
    cflSides
    {
       type            fixedValue;
       value           uniform (0 0 0);
    }        
    air_to_interconnect0
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    air_to_electrolyte //cathode
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    
}


