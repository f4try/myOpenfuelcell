FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.001";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 1000;

boundaryField
{
    aflSides
    {
        type            zeroGradient;
    }
    anodeSides
    {
        type            zeroGradient;
    }
    fuelInlet
    {
        type            fixedValue;
        value           uniform 1000;
    }
    fuelOutlet
    {
        type            zeroGradient;
    }
    fuel_to_interconnect1
    {
        type            calculated;
        value           uniform 0;
    }
    fuel_to_electrolyte
    {
        type            calculated;
        value           uniform 0;
    }
}


