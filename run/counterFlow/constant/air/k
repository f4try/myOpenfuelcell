FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.001";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 1 -3 -1 0 0 0];

internalField   uniform 0;

boundaryField
{
    airInlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    airOutlet
    {
        type            zeroGradient;
    }
    cathodeSides
    {
        type            zeroGradient;
    }
    cflSides
    {
        type            zeroGradient;
    }
    air_to_interconnect0
    {
        type            calculated;
        value           uniform 0;
    }
    air_to_electrolyte
    {
        type            calculated;
        value           uniform 0;
    }
}


